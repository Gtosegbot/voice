# =======================
# Configurações do WhatsApp
# =======================
WHATSAPP_TOKEN=seu_token_aqui
WHATSAPP_PHONE_NUMBER_ID=seu_id_aqui
WHATSAPP_BUSINESS_ACCOUNT_ID=seu_id_aqui

# =======================
# Configurações do n8n
# =======================
N8N_URL=https://seu-n8n.com
N8N_API_KEY=sua_chave_api

# =======================
# Configurações do Banco de Dados
# =======================
DATABASE_URL=postgresql://usuario:senha@localhost:5432/nome_do_banco

# =======================
# Configurações de Autenticação
# =======================
JWT_SECRET=sua_chave_secreta_muito_segura
JWT_ALGORITHM=HS256

# =======================
# Configurações Gerais
# =======================
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=INFO
LOG_FILE=app.log

# =======================
# Configuração de CORS (origens permitidas)
# =======================
CORS_ORIGINS=http://localhost:3000,https://suaempresa.com.br

# =======================
# Configuração de Rate Limit
# =======================
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# =======================
# Configurações de E-mail (SMTP)
# =======================
EMAIL_API_KEY=sua-chave-email
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=seu-email@gmail.com
SMTP_PASSWORD=sua-senha-email
EMAIL_FROM=no-reply@suaempresa.com.br

# =======================
# Configurações de Armazenamento (exemplo: AWS S3)
# =======================
STORAGE_BUCKET=nome-do-bucket
STORAGE_REGION=sa-east-1
STORAGE_ACCESS_KEY=sua-chave-acesso
STORAGE_SECRET_KEY=sua-chave-secreta

# =======================
# Configurações de Cache (exemplo: Redis)
# =======================
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=sua-senha-redis

# =======================
# Configuração de Supabase (opcional)
# =======================
SUPABASE_URL=sua-url-supabase
SUPABASE_KEY=sua-chave-supabase
SUPABASE_JWT_SECRET=sua_supabase_jwt_secret

# =======================
# Configurações de LLMs (Roteador, OpenAI, Anthropic, etc)
# Use o CONDUCTOR como roteador principal para LLMs ou defina o modelo padrão
# =======================
MODEL_PROVIDER=conductor
CONDUCTOR_API_KEY=sua-chave-conductor
CONDUCTOR_ENDPOINT=https://conductor.arcee.ai/openai/v1/chat/completions

# OpenAI (GPT)
OPENAI_API_KEY=sua-chave-openai
OPENAI_ENDPOINT=https://api.openai.com/v1/chat/completions

# Anthropic (Claude)
ANTHROPIC_API_KEY=sua-chave-anthropic
ANTHROPIC_ENDPOINT=https://api.anthropic.com/v1/messages

# Google (Gemini)
GOOGLE_API_KEY=sua-chave-google
GOOGLE_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent

# Cohere
COHERE_API_KEY=sua-chave-cohere
COHERE_ENDPOINT=https://api.cohere.ai/v1/generate

# Mistral AI
MISTRAL_API_KEY=sua-chave-mistral
MISTRAL_ENDPOINT=https://api.mistral.ai/v1/chat/completions

# Together AI
TOGETHER_API_KEY=sua-chave-together
TOGETHER_ENDPOINT=https://api.together.xyz/v1/completions

# (Adicione outras plataformas de LLM conforme necessário)

# =======================
# Configurações de TTS (geração de voz)
# =======================
ELEVENLABS_API_KEY=sua-chave-elevenlabs
ELEVENLABS_DEFAULT_VOICE=eleven_multilingual_v2
AZURE_TTS_KEY=sua-chave-azure-tts
AZURE_TTS_REGION=sua-regiao-azure
AZURE_TTS_DEFAULT_VOICE=pt-BR-FranciscaNeural
GOOGLE_TTS_KEY=sua-chave-google-tts
GOOGLE_TTS_DEFAULT_VOICE=pt-BR-Wavenet-A

# =======================
# Defina modelos fixos por canal se necessário (opcional)
# =======================
CALLS_LLM_MODEL=
EMAIL_LLM_MODEL=
SMS_LLM_MODEL=
CHAT_LLM_MODEL=

# =======================
# Outros canais ou integrações podem ser adicionados conforme a escala do projeto!
# =======================
